function restyle() {
    var logo = document.getElementById('logo');
    var separator = document.getElementById('separation_text');
    if(separator) { separator.innerHTML = "<span style=\"background: #fff;margin-top: -0.8em;padding: 0 0.5em;\">OR</span>"; }
}

