<script>
document.getElementsByClassName("closesubbtn")[0].innerHTML = "<i class='ion-arrow-left-c'></i>";
document.getElementsByClassName("closebtn")[0].innerHTML = "<i class='ion-close-round'></i>";

var mmSubContent = document.createElement('ul');
function openNav() {
    document.getElementById("mySidenav").style.display = "block";
document.getElementsByTagName("body")[0].style.overflow = "hidden";

document.getElementById("mySidenav").style.overflow = "scroll";
    setTimeout(function() { document.getElementById("mySidenav").style.marginRight = "0%"; }, 150);
    
    //document.getElementById("main").style.marginRight = "100%";
}
function openSubNav(subID) {
    mmSubContent.innerHTML = mmSubs[subID].toString();
    document.getElementById('mySubnav').appendChild(mmSubContent);  
    document.getElementById("mySubnav").style.marginRight = "0%";
}
function closeNav() {
try{document.getElementById("mySubnav").style.marginRight = "-100%";
document.getElementsByTagName("body")[0].style.overflow = "scroll";
    
setTimeout(function() { document.getElementById("mySidenav").style.display = "none"; }, 500);
}
catch(err){}
    document.getElementById("mySidenav").style.marginRight = "-100%";;
    //document.getElementById("main").style.marginRight= "0";
}
function closeSubNav() {
    document.getElementById("mySubnav").style.marginRight = "-100%";;
}
</script>
