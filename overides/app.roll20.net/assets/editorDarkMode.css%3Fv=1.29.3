:root {
  --dark-background: #1C1C1C;
  --dark-card-purple: #2d123a;
  --dark-proficiency-card: #212b30;
  --dark-warning-card: #302b21;
  --dark-green-d60: #28794c;
  --dark-green-l20: #48BD7D;
  --dark-error-red: #572124;

  --bg-av-controls: rgba(0, 0, 0, 0.5);
  --network-icon-color: rgba(255, 255, 255, 0.5);
	--colors-dark-f10: rgba(255, 255, 255, 0.1);
}

/* || GLOBAL DEFAULTS  */

body {
  font-family: "Proxima Nova", "ProximaNova-Regular", -apple-system,
    BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell,
    "Helvetica Neue", sans-serif;
  font-size: 13px;
  letter-spacing: 0px;
  line-height: 18px;
  font-weight: 400;
}

p {
  font-family: "Proxima Nova", "ProximaNova-Regular", -apple-system,
    BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell,
    "Helvetica Neue", sans-serif;
  font-size: 13px;
  letter-spacing: 0px;
  line-height: 18px;
  font-weight: 400;
}

body {
  background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.2) 100%), var(--dark-background);
}

body {
  color: var(--grayscale-dark-base);
}

h1,
h2,
h3,
h4,
h5,
h6,
blockquote,
.note-btn-group .dropdown-style>li>a>p {
  color: var(--grayscale-dark-base);
}

.btn,
.btn.btn-default {
  box-shadow: none;
  background: var(--primary-dark);
  text-shadow: none;
  border: none;
  padding: 6px;
  color: var(--grayscale-dark-base);
}

.btn-group .btn.btn-default {
  background-color: var(--primary-dark);
  outline: 1px solid var(--dark-background);
}

.btn-primary {
  background: var(--primary-dark-l20);
  outline: 1px solid var(--primary-dark);
}

.btn:hover,
.btn:focus,
.btn:active,
.btn.active,
.btn.btn-default:hover,
.btn.btn-default:focus,
.btn.btn-default:active,
.btn-primary:hover,
.btn-primary:focus,
.btn-primary:active {
  color: var(--grayscale-dark-base);
  background: var(--primary-dark-l20);
  border: none;
}

.btn:focus,
.btn.btn-default:focus,
.btn-primary:focus {
  outline: 2px solid var(--primary-dark-l40);
  outline-offset: -2px;
}

btn.btn-primary:active {
  background: var(--primary-dark);
}

input,
textarea,
select,
.uneditable-input {
  background: none;
  border: 1px solid var(--grayscale-dark-d30);
  border-radius: 8px;
  color: var(--grayscale-dark-d30);
}

input:hover,
textarea:hover,
select:hover {
  border-color: var(--primary-dark-l40);
}

input:focus,
textarea:focus,
select:focus {
  box-shadow: none;
  outline: none;
  border-color: var(--primary-dark-l40);
  outline: 1px solid var(--primary-dark-l40);
}

label {
  color: var(--grayscale-dark-d30);
}

select > option {
  font-family: "Proxima Nova", "ProximaNova-Regular", -apple-system,
    BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell,
    "Helvetica Neue", sans-serif;
  background-color: var(--surface-dark2);
  color: var(--grayscale-dark-d30);
}

code {
  color: var(--grayscale-dark-base);
  background-color: var(--primary-dark);
}

/*  ||  MODALS      */

.popoutwindow>.ui-dialog {
  padding: 0;
  border-radius: 0 !important;
}

.ui-dialog {
  background: var(--surface-dark);
  border-radius: 14px;
  border: none;
  box-shadow: 0px 5px 20px #000;
}

.ui-dialog .ui-dialog-titlebar {
  border: none;
}

.ui-dialog .ui-dialog-title {
  color: var(--grayscale-dark-base);
}

.ui-dialog .ui-dialog-buttonpane {
  background: none;
  border: none;
  box-shadow: none;
}

.ui-dialog .ui-dialog-titlebar a:not(.btn-link):not(.advanced_charsheet__header__nav__tabs_link),
.ui-dialog a.ui-dialog-titlebar-close {
  filter: invert(1);
}

.ui-dialog .ui-dialog-buttonpane button {
  box-shadow: none;
  background: var(--primary-dark);
  border: none;
  color: var(--grayscale-dark-base);
  padding: 8px;
  border-radius: 6px;
}

.ui-dialog .ui-dialog-buttonpane button:hover,
.ui-dialog .ui-dialog-buttonpane button:focus,
.ui-dialog .ui-dialog-buttonpane button:active {
  outline: 2px solid var(--primary-dark-l40);
  outline-offset: -2px;
}

.ui-dialog .ui-dialog-buttonpane button:hover {
  background: var(--primary-dark-l20);
}

.modal-content {
  background-color: var(--surface-dark);
}

.form-control {
  color: var(--color-surface2);
}

.note-btn-group .dropdown-menu {
  background-color: var(--surface-dark);
}

.note-table>.note-dimension-display {
  color: var(--grayscale-dark-base);
}

.note-palette-title {
  color: var(--grayscale-dark-base);
}

.chosen-container-multi .chosen-choices {
  color: var(--grayscale-dark-d30);
  background-color: transparent;
  background-image: none;
}

.chosen-container-multi .chosen-choices li.search-field input[type="text"] {
  color: var(--grayscale-dark-d30);
}

.chosen-container .chosen-results {
  color: var(--grayscale-dark-base);
  background-color: var(--surface-dark);
}

/*  ||  VTT STYLING   */

#editor-wrapper {
  background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.2) 100%), var(--dark-background);
}

.ui-widget-content {
  color: var(--grayscale-dark-d30);
  background-color: var(--surface-dark-dark);
  background-image: none;
}

#zoomclick {
  filter: drop-shadow(0px 0px 6px rgb(0, 0, 0, 0.5));
  height: 198px;
}

#zoomclick .btn {
  background-color: var(--primary-dark);
  box-shadow: none;
  background-image: none;
  padding: 9px;
  border: none;
}

.zoomValue {
  color: var(--grayscale-dark-base);
}

#zoomclick .btn:hover {
  background-color: var(--primary-dark-l20);
}

#zoomclick .btn.zoomminus {
  margin-top: 1px;
}

#zoomclick .btn::after,
#zoomclick .btn::before {
  background-color: var(--grayscale-dark-d30);
}

#zoomslider {
  border: none;
  background: var(--grayscale-d60);
  width: 8px !important;
  border-radius: 0;
  margin-top: 1px;
}

#zoomslider a.ui-slider-handle {
  background: none;
  border: none;
  color: var(--grayscale-dark-base);
  text-align: center;
  font-size: 20px;
  padding-left: 5px;
  margin-left: 2px;
  margin-bottom: 0px;
  box-shadow: none;
  height: 16px !important;
}

#zoomslider a.ui-slider-handle::after {
  content: "\25CF";
}

/*  ||  TOOLBAR  */

#floatingtoolbar {
  border-color: var(--grayscale-d60);
  box-shadow: 1px 2px 5px rgb(0, 0, 0, 0.5);
}

#floatingtoolbar li {
  background-color: var(--surface-dark2);
  color: var(--grayscale-dark-d30);
  border-color: var(--surface-dark);
}

#floatingtoolbar li.activebutton {
  background-color: var(--primary-dark);
  color: var(--grayscale-dark-base);
}

#floatingtoolbar li:hover,
#floatingtoolbar li.select:hover {
  background-color: var(--primary-dark-l20);
  color: var(--primary-dark-text);
}

#floatingtoolbar div.submenu ul {
  border: none;
  background-color: var(--surface-dark);
  border-radius: 14px;
}

#floatingtoolbar div.submenu ul li:first-of-type {
  border-top-left-radius: 11px;
  border-top-right-radius: 11px;
}

#floatingtoolbar div.submenu ul li:last-of-type {
  border-bottom-left-radius: 11px;
  border-bottom-right-radius: 11px;
}

#editinglayer li.gm_slider_box {
  background-color: var(--surface-dark2);
}

#editinglayer li.gm_slider_box img {
  filter: invert(1);
}

#secondary-toolbar {
  background-color: var(--surface-dark2);
  color: var(--primary-dark-text);
  opacity: 0.85;
  box-shadow: 1px 1px 3px #222;
}

div#color_selector {
  background-color: var(--surface-dark2);
  border-color: var(--grayscale-d60);
}

div#color_custom input {
  padding-left: 6px;
}

/*  || PAGE MENU   */

#page-toolbar {
  background-color: var(--surface-dark);
}

#page-toolbar .handle {
  background-color: var(--primary-dark);
  color: var(--grayscale-dark-base);
}

#page-toolbar .handle:hover,
#page-toolbar .handle:focus {
  background-color: var(--primary-dark-l20);
}

#page-toolbar .handle:focus {
  outline: 1px solid var(--primary-dark-l40);
}

#page-toolbar .activepage img.pagethumb {
  border-color: var(--primary-dark-l20);
}

#shortcut-help dd {
  color: var(--grayscale-dark-d30);
}

.pagedetails .disable_box,
.pagedetails input[type="text"],
.tokeneditor .disable_box {
  background-color: var(--grayscale-d90);
}

.tokeneditor__dropdown .dropdown-menu {
  background-color: var(--dark-background);
}

.section.tracks .result .controls .selectize-input, .selectize-control.single .selectize-input.input-active {
  color: var(--grayscale-dark-base);
  background-color: var(--primary-dark);
  background-image: none;
}

.tokeneditor__bar-inputs input[type="text"][disabled],
.token_bar_input[disabled] {
  background-color: var(--grayscale-d90);
  border-color: initial;
}

.ui-dialog-buttonpane .ui-dialog-buttonset .ui-button {
  background-image: none;
  background-color: var(--primary-dark);
}

.ui-dialog-buttonset .ui-button:nth-child(1),
.ui-dialog-buttonset .ui-button:nth-child(2) {
  background-image: none;
  background-color: var(--surface-dark);
  color: var(--grayscale-dark-base);
  border: 1px solid var(--primary-dark);
}

.ui-dialog-buttonset .ui-button:nth-child(3) {
  color: #ffffff;
  outline: 2px solid var(--dark-purple-text);
  outline-offset: -2px;
}

.zoomoptionsmenu > * {
  background-color: var(--primary-dark);
  border-radius: 10px;
}

/*  || RIGHT SIDEBAR - GENERAL  */

#rightsidebar {
  background-color: var(--surface-dark);
}

.ui-tabs .ui-tabs-panel {
  background: var(--surface-dark);
}

#rightsidebar ul.tabmenu {
  background-color: var(--surface-dark);
}

#rightsidebar ul.tabmenu li a {
  color: var(--grayscale-dark-d30);
}

.ui-tabs .ui-tabs-nav li:hover {
  background: none;
  border-bottom: none;
}

#sidebarcontrol {
  background-color: gray;
}

.dd-content {
  color: var(--grayscale-dark-base);
  font-weight: normal;
  border-color: var(--grayscale-d60) !important;
}

.dd-list .handout img[src="/images/handout.png"],
.archivetable img {
  filter: invert(0.87);
}

.inapp .sourcefooter {
  filter: invert(0.87);
}

.table-striped > tbody > tr:nth-of-type(2n + 1) {
  background-color: var(--surface-dark2);
}

.table > thead > tr > th,
.table > tbody > tr > th,
.table > tfoot > tr > th,
.table > thead > tr > td,
.table > tbody > tr > td,
.table > tfoot > tr > td {
  border-top: var(--grayscale-d60);
}

#rightsidebar .paddedtable button {
  padding-top: 5px;
  padding-bottom: 5px;
}

#rightsidebar .paddedtable button:not(:last-child) {
  margin-bottom: 2px;
}

#rightsidebar .ui-tabs-nav li.ui-state-active a {
  color: var(--grayscale-dark-base);
  background-color: var(--primary-dark-l20);
}

.ui-tabs .ui-tabs-nav {
  border-bottom: none;
}

/*  || RIGHT SIDEBAR - TEXT CHAT    */

.textchatcontainer .message {
  background-color: var(--surface-dark);
}

.textchatcontainer .message.you {
  background-color: var(--surface-dark2);
}

.textchatcontainer .message.private {
  background-color: var(--surface-dark-gamecard);
}

.textchatcontainer .message.system {
  background-color: var(--dark-card-purple);
  color: var(--grayscale-dark-d30);
}

.textchatcontainer .message.emote{
  background-color: var(--surface-dark2);
  color: var(--yellow-base);
}

.textchatcontainer .error{
  background-color: var(--dark-error-red);
}
#textchat-input {
  background-color: var(--surface-dark);
  border-color: #202020;
  color: var(--grayscale-dark-base);
}

.textchatcontainer .message .spacer {
  background-color: var(--dark-background);
}

.textchatcontainer .message.private .spacer {
  background-color: var(--dark-background);
}

.textchatcontainer .message.you .spacer {
  background-color: var(--dark-background);
}

.sheet-darkmode.textchatcontainer .formula,
.sheet-darkmode.textchatcontainer .rolled {
  background-color: var(--dark-background);
  color: var(--grayscale-dark-base);
  font-weight: normal;
  border-color: var(--primary-dark);
  padding: 6px;
  border-radius: 8px;
  box-decoration-break: clone;
}

.sheet-darkmode.textchatcontainer .formula span,
.sheet-darkmode.textchatcontainer .rolled span{
-webkit-text-stroke: 1px darkgray;
text-stroke: 1px darkgray;
}

.diceroll .didroll {
  color: var(--grayscale-dark-base);
  text-shadow: none;
  text-shadow: -4px -4px 4px var(--dark-background),
    4px -4px 4px var(--dark-background), -4px 4px 4px var(--dark-background),
    4px 4px 4px var(--dark-background);
}

#dicerollerdialog > a {
  color: var(--grayscale-dark-d30);
}

#textchat .userscript-sharelink {
  background-color: var(--primary-dark-l40);
}

#textchat .userscript-sharelink a {
  color: var(--primary-dark-l40) !important;
}

.userscript-commandintro strong {
  color: var(--primary-dark-l40);
}

.userscript-commandintro img {
  content: url("/v2/images/roll20-logo-darktheme.png");
}

/*  || RIGHT SIDEBAR - IMAGE LIBRARY  */

#imagedialog .tabcontainer a {
  color: var(--grayscale-dark-d30);
}

#imagedialog .tabcontainer a:hover {
  color: var(--primary-dark-l60);
}

#imagedialog .tabcontainer a.active {
  color: var(--primary-dark-l40);
  text-decoration: underline;
  text-underline-offset: 3px;
}

#imagedialog .searchbox {
  background: none;
}

#imagedialog .searchbox input:focus {
}

/*  || RIGHT SIDEBAR - JOURNAL  */
.dd-item .playerdots {
  background: none;
}

.searchbox a {
  color: var(--grayscale-dark-base);
}

#imagedialog #librarysearchroot .googleresult {
  background-color: var(--surface-dark-gamecard);
}

#imagedialog #librarysearchroot .googleresult a {
  color: var(--grayscale-dark-d30);
}

/*  || RIGHT SIDEBAR - COMPENDIUM */
.compendium-removed-expansions {
  background-color: var(--surface-dark2);
  color: var(--grayscale-dark-base);
  border-color: var(--grayscale-d60);
}
/* || RIGHT SIDEBAR - JUKEBOX  */
#jukebox .primarycontrols .btn,
#jukebox .secondarycontrols .btn {
  background-image: none;
}
#jukebox .primarycontrols .btn:active,
#jukebox .secondarycontrols .btn:active {
  background-image: none;
}

.uppertab {
  background-color: var(--primary-dark);
  color: var(--grayscale-dark-base);
}

.uppertab.active {
  background-color: var(--primary-dark-l20);
  color: var(--grayscale-dark-base);
}

.uppertab.active:hover {
  background-color: var(--primary-dark-l20);
  outline: 2px solid var(--primary-dark-l20);
  outline-offset: -2px;
}

#jukeboxfolderroot a.play,
#jukeboxfolderroot a.exportplaylist,
#jukeboxfolderroot a.playstyle,
#jukeboxfolderroot a.jukeboxitem-menu,
#jukeboxwhatsplaying a.play {
